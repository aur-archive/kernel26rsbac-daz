# mkinitcpio preset file for kernel26rsbac-daz

LOCAL_VERSION=rsbac-daz

########################################
# DO NOT EDIT THIS LINE:
source /etc/mkinitcpio.d/kernel26$LOCAL_VERSION.kver
########################################
ALL_config="/etc/mkinitcpio.conf"

PRESETS=('default' 'fallback')

default_image="/boot/kernel26$LOCAL_VERSION.img"

fallback_image="/boot/kernel26$LOCAL_VERSION-fallback.img" 
fallback_options="-S autodetect"
